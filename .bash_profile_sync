tmux attach -t working
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias libreoffice="libreoffice5.0"
alias today="date +%Y-%m-%d"
alias d="du -hs *"
alias yesterday="date -d'yesterday' +%Y-%m-%d"
alias tomorrow="date -d'tomorrow' +%Y-%m-%d"
alias cp="cp -i" # Confirm before overwriting
alias mv="mv -i"
alias rm="rm -I" # Prompt before large/recursive deletions
export EDITOR=vim
# set gtk im module to default (to make synapse work)
export GTK_IM_MODULE=" "
